{
  "name": "fixpack",
  "description": "cli tool that cleans up package.json files.",
  "version": "2.3.0",
  "author": "Henrik Joreteg <henrik@andyet.net>",
  "bin": {
    "fixpack": "dist/bin.js"
  },
  "bugs": {
    "url": "https://github.com/henrikjoreteg/fixpack/issues"
  },
  "dependencies": {
    "alce": "1.0.0",
    "colors": "*",
    "extend-object": "^1.0.0",
    "rc": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.22.0",
    "rimraf": "2.5.4",
    "standard": "^3.2.1"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/henrikjoreteg/fixpack",
  "keywords": [
    "cleanup",
    "package"
  ],
  "license": "MIT",
  "main": "fixpack",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/henrikjoreteg/fixpack"
  },
  "scripts": {
    "build": "babel --copy-files --out-dir ./dist --ignore node_modules ./src",
    "prebuild": "rimraf dist",
    "prepack": "npm run build",
    "test": "standard src"
  }
}
